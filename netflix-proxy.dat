#cloud-config
apt_upgrade: true
runcmd:
 - [ printf, "'Scheduling WAAgent upgrade...\n'" ]
 - [ printf, "'sudo rm -rf /opt/WALinuxAgent; sudo apt-get -y install git; sudo git clone https://github.com/Azure/WALinuxAgent.git /opt/WALinuxAgent; sudo /opt/WALinuxAgent/get-agent.py; sudo service walinuxagent restart; sudo waagent -version' | at now" ]
 - [ sudo, "git clone https://github.com/ab77/netflix-proxy /opt/netflix-proxy && /opt/netflix-proxy/build.sh && sudo docker restart bind" ]
