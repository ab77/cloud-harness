#cloud-config
apt_upgrade: true
runcmd:
  - [ sudo, "apt-get -y install git" ]
  - [ sudo, "git clone https://github.com/ab77/netflix-proxy /opt/netflix-proxy && /opt/netflix-proxy/build.sh" ]
  - [ printf, "'sudo rm -rf /opt/WALinuxAgent; sudo git clone https://github.com/Azure/WALinuxAgent.git /opt/WALinuxAgent; sudo /opt/WALinuxAgent/get-agent.py; sudo service walinuxagent restart' | at now" ]
  - [ sudo, "docker restart bind" ]
